  <ItemGroup>
    <None Remove="Imagenes\actualizar.PNG" />
    <None Remove="Imagenes\cliente.PNG" />
    <None Remove="Imagenes\crear.PNG" />
    <None Remove="Imagenes\eliminar.PNG" />
    <None Remove="Imagenes\interfaz.PNG" />
    <None Remove="Imagenes\leer.PNG" />
